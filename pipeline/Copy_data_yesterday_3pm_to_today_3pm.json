							},
							"modifiedDatetimeEnd": {
								"value": "@addHours(startofday(utcnow()), 15)",
								"type": "Expression"
							},
							"wildcardFileName": "*",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Input_copy_yesterday_3pm_to_today_3pm",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "output_copy_yesterday_3pm_to_today_3pm",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Copy_specifice_time_period_data"
		},
		"annotations": []
	}
